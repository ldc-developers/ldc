// This configuration file uses libconfig.
// See http://www.hyperrealm.com/libconfig/ for syntax details.

// The default group is required
default:
{
    // 'switches' holds array of string that are appends to the command line
    // arguments before they are parsed.
    switches = [
        "-I@CHROOT_INSTALL_DIR@@INCLUDE_INSTALL_DIR@",
        "-I@CHROOT_INSTALL_DIR@@INCLUDE_INSTALL_DIR@/@RUNTIME_AIO@",
        "-I@CHROOT_INSTALL_DIR@@INCLUDE_INSTALL_DIR@/ldc",
        "-L-L@CHROOT_INSTALL_DIR@@CMAKE_INSTALL_LIBDIR@",
        "-defaultlib=@RUNTIME_AIO@",
        "-debuglib=@RUNTIME_AIO@"
    ];
};
